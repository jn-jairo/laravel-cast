{
    "name": "jn-jairo/laravel-cast",
    "description": "Cast for Laravel.",
    "keywords": [
        "jn-jairo",
        "laravel-cast"
    ],
    "homepage": "https://github.com/jn-jairo/laravel-cast",
    "license": "MIT",
    "authors": [
        {
            "name": "Jairo Correa",
            "email": "jn.j41r0@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/encryption": "^5.8|^6.0|^7.0|^8.0|^9.0",
        "illuminate/support": "^5.8|^6.0|^7.0|^8.0|^9.0",
        "ramsey/uuid": "^3.8|^4.0",
        "thecodingmachine/safe": "^1.1|^2.1"
    },
    "require-dev": {
        "moontoast/math": "^1.1",
        "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0|^7.0",
        "php-decimal/php-decimal": "^1.1",
        "phpunit/phpunit": "^8.3|^9.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "suggest": {
        "php-decimal/php-decimal": "Required to use decimal type (^1.1).",
        "moontoast/math": "Required to use ordered UUIDs (^1.1)."
    },
    "autoload": {
        "psr-4": {
            "JnJairo\\Laravel\\Cast\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JnJairo\\Laravel\\Cast\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit",
        "test-stable": "for version in 7.2 7.3 7.4 8.0 8.1 ; do if command -v php${version} > /dev/null 2>&1 ; then echo '\nPHP: '${version}'\n' && php${version} $(which composer) update --prefer-dist --no-interaction --prefer-stable > /dev/null 2>&1 && php${version} vendor/bin/phpunit ; fi ; done",
        "test-lowest": "for version in 7.2 7.3 7.4 ; do if command -v php${version} > /dev/null 2>&1 ; then echo '\nPHP: '${version}'\n' && php${version} $(which composer) update --prefer-dist --no-interaction --prefer-lowest --prefer-stable > /dev/null 2>&1 && php${version} vendor/bin/phpunit ; fi ; done",
        "code-style": "vendor/bin/phpcs -n src/ tests/ config/"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "JnJairo\\Laravel\\Cast\\CastServiceProvider"
            ],
            "aliases": {
                "Cast": "JnJairo\\Laravel\\Cast\\Facades\\Cast"
            }
        }
    }
}
